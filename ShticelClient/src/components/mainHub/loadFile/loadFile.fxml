<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="1098.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.mainHub.loadFile.LoadFileController">
   <children>
      <Button fx:id="loadFileButton" ellipsisString="" minHeight="35.0" minWidth="150.0" onAction="#loadFileButtonAction" styleClass="button" text="Load Sheet File" textAlignment="CENTER" HBox.hgrow="NEVER">
         <font>
            <Font size="16.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Button>
      <StackPane alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="590.0" HBox.hgrow="ALWAYS">
         <children>
            <Label id="load-file-ta" fx:id="loadFileTA" prefWidth="1100.0" visible="false" />
         </children>
         <HBox.margin>
            <Insets left="15.0" />
         </HBox.margin>
      </StackPane>
      <Button fx:id="chatRoomButton" mnemonicParsing="false" onAction="#chatRoomButtonAction" text="Chat Room" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets left="160.0" />
         </HBox.margin>
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets right="20.0" />
   </padding>
</HBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="1280.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="subComponents.actionLine.ActionLineController">
   <children>
      <Label id="selected-cell-id" fx:id="selectedCellID" minWidth="140.0" text="Selected Cell ID" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <padding>
            <Insets left="5.0" />
         </padding>
         <styleClass>
            <String fx:value="selected-cell" />
            <String fx:value="action-line" />
         </styleClass>
      </Label>
      <Label id="selected-cell-ta" fx:id="selectedCellTA" minWidth="30.0" text="A5" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="15.0" />
         </HBox.margin>
         <font>
            <Font size="18.0" />
         </font>
         <styleClass>
            <String fx:value="selected-cell" />
            <String fx:value="action-line" />
         </styleClass>
      </Label>
      <Label id="original-value-header" minWidth="125.0" text="Original Value" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <HBox.margin>
            <Insets left="25.0" />
         </HBox.margin>
         <styleClass>
            <String fx:value="original-value" />
            <String fx:value="action-line" />
         </styleClass>
      </Label>
      <TextField id="original-value-tf" fx:id="originalValueTF" maxWidth="350.0" prefWidth="250.0" styleClass="action-line" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="15.0" />
         </HBox.margin>
      </TextField>
      <Button id="update-value-button" fx:id="updateValueButton" minWidth="120.0" mnemonicParsing="false" styleClass="action-line" text="Update Value" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <HBox.margin>
            <Insets left="15.0" />
         </HBox.margin>
      </Button>
      <Label minWidth="210.0" styleClass="action-line" text="Last Update Cell Version" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="55.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label styleClass="action-line" text="1" HBox.hgrow="ALWAYS">
         <font>
            <Font size="18.0" />
         </font>
         <HBox.margin>
            <Insets left="15.0" />
         </HBox.margin>
      </Label>
      <Region prefWidth="10.0" HBox.hgrow="ALWAYS" />
      <Button id="version-selector-button" fx:id="versionSelectorButton" alignment="CENTER_RIGHT" minWidth="135.0" mnemonicParsing="false" text="Version Selector" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <HBox.margin>
            <Insets left="100.0" />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets left="15.0" right="20.0" top="5.0" />
   </padding>
</HBox>
